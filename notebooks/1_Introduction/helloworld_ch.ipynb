   "source": [
    "# 引入TensorFlow库\n",
    "import tensorflow as tf\n",
    "\n",
    "# 创建一个常量\n",
    "hello = tf.constant(\"Hello World\")\n",
    "\n",
    "# 打印一下试试\n",
    "print(hello)\n",
    "\n",
    "# 创建一个Session\n",
    "sess = tf.Session()\n",
    "\n",
    "# 用session运行\n",
    "print(sess.run(hello))\n"
   ]
